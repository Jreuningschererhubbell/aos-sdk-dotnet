{
	"metadata": [
		{
			"src": [
				{
					"files": [
						"Aetheros.OneM2M.Binding/*.csproj",
						"Aetheros.OneM2M.Api/*.csproj"
					],
					"src": ".."
				}
			],
			"dest": "api",
			"filter": "filterConfig.yml"
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"articles/**.md",
					"articles/**/toc.yml",
					"toc.yml",
					"*.md"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**",
					"wwwroot/**"
				]
			}
		],
		"dest": "wwwroot",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"statictoc",
			"templates/default"
		],
		"globalMetadata": {
			"_appTitle": "AOS oneM2M SDK",
			"_appFooter": "<span>Copyright &copy; Aetheros, Inc.</span>",
			"_appLogoPath": "images/Aetheros.png",
			"_appFaviconPath": "images/Aetheros.png"
		},
		"postProcessors": [],
		"markdownEngineName": "markdig",
		"noLangKeyword": false
	}
}